#!/bin/bash

xsetroot -cursor_name left_ptr &

xrandr --output HDMI-A-1 --auto --primary --output HDMI-A-0 --off --output DisplayPort-0 --off

feh --bg-fill $HOME/.background-image &

#setxkbmap -option compose:caps
xmodmap ~/.Xmodmap

#xinput --set-prop "pointer:Razer Razer Basilisk V3" 'libinput Accel Speed' -0.7

xscreensaver -no-splash &

signal-desktop &
nextcloud &
goldendict &
